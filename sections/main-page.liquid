{{ 'main-page.css' | asset_url | stylesheet_tag }}

<div class="main_page">
  <div class="content_wrapper">
    <div class="page_head">
      <h1>{{ page.title }}</h1>
    </div>
    <div
      class="page_content"
    >
      <div
        class="content"
        {% if page.url contains 'store-locator' %}
          style="max-width: 1200px; width: 100%;"
        {% endif %}
      >
        {{ page.content }}
      </div>
    </div>
  </div>
</div>
